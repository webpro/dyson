{
  "name": "dyson",
  "version": "2.0.0",
  "description": "Node server for dynamic, fake JSON.",
  "keywords": [
    "API",
    "JSON",
    "REST",
    "data",
    "dummy",
    "dynamic",
    "fake",
    "generator",
    "proxy",
    "response",
    "server"
  ],
  "author": {
    "email": "lars@webpro.nl",
    "name": "Lars Kappert"
  },
  "main": "./lib/dyson.js",
  "bin": {
    "dyson": "./bin/dyson.js"
  },
  "scripts": {
    "test": "ava test/*.js",
    "lint": "eslint lib test",
    "format": "prettier --write \"{bin,lib,test}/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webpro/dyson"
  },
  "files": [
    "bin",
    "lib"
  ],
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "body-parser": "1.17.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "express": "4.16.2",
    "lodash": "4.17.4",
    "request": "2.81.0",
    "require-directory": "2.1.1",
    "serve-favicon": "2.4.5"
  },
  "devDependencies": {
    "@babel/plugin-syntax-object-rest-spread": "7.0.0-beta.32",
    "ava": "0.23.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "dyson-generators": "0.1.1",
    "dyson-image": "0.1.3",
    "eslint": "4.7.1",
    "eslint-config-prettier": "2.5.0",
    "eslint-plugin-prettier": "2.3.1",
    "prettier": "1.7.0",
    "sinon": "4.1.2",
    "supertest": "3.0.0"
  },
  "ava": {
    "babel": {
      "presets": [
        "@ava/stage-4",
        "@ava/transform-test-files"
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
  },
  "license": "MIT"
}
